{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    "stage-1",
    "stage-2"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "comments": false,
      "plugins": [
        "lodash",
        "transform-remove-console"
      ]
    }
  }
}
